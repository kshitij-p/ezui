{
  "name": "@ezui/cli",
  "version": "0.4.0",
  "main": "./dist/index.js",
  "bin": {
    "ezui": "./dist/index.js"
  },
  "keywords": [
    "cli",
    "react component library",
    "ezui"
  ],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --dts && node ./dist/index.js",
    "devAdd": "tsup src/index.ts --dts && node ./dist/index.js add",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "pnpm build && vitest run --coverage"
  },
  "devDependencies": {
    "@ezui/tsconfig": "workspace:*",
    "@types/prompts": "^2.4.4",
    "@vitest/coverage-c8": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-config-ezui": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3",
    "vitest": "^0.31.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "prompts": "^2.4.2",
    "puppeteer": "^20.4.0",
    "zod": "^3.21.4"
  }
}
